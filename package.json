{
  "name": "preact-ex",
  "version": "0.0.0",
  "description": "",
  "author": "rafamel",
  "private": true,
  "scripts": {
    "precommit": "MSG='Committing in:' npm run validate",
    "build": "run-s validate copy && parcel build src/index.js -d ./build",
    "dev": "(rm -r .cache dist || exit 0) && (parcel watch src/index.html --public-url ./ & live-server ./dist --port=5000 --no-browser & nodemon -w ./src -e js,scss,html -x \"clear && npm run lint\")",
    "fix": "prettier --write \"src/**/*.{js,scss,md}\"",
    "lint": "eslint ./src --ext .js",
    "validate": "run-s fix lint && npm run outdated",
    "outdated": "if !(npm outdated) && [ -n \"$MSG\" ]; then printf \"\n\" && for ((i=10; i>=0; --i)); do sleep 1 && printf '\r%s %2d' \"$MSG\" \"$i\"; done; fi;",
    "copy": "cp -r node_modules/react-svg-inline vendor/ && rm vendor/*/package.json"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "react-svg-inline": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-node-modules": "0.0.1",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-preact": "^1.1.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-react-app": "^3.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.1",
    "parcel-bundler": "^1.6.2",
    "prettier": "1.10.2"
  }
}